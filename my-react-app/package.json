{
  "name": "my-react-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron-dev": "concurrently \"npm run dev\" \"nodemon --exec electron .\"",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron-start": "electron .",
    "electron-pack": "npm run build && electron-builder --dir",
    "electron-dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.0",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3",
    "vite": "^5.2.0"
  },
  "build": {
    "appId": "com.VArunbuilds.Jobtracking",
    "productName": "Jobapplication-tracking ",
    "asar": true,
    "files": [
      "dist/**/*",
      "electron.js",
      "preload.js",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  },
    "nodemonConfig": {
  
}
}